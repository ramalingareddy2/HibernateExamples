<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mtml.entities">
	<class name="Passenger" table="passenger">
		<id name="passengerNo" column="passenger_no">
			<generator class="increment"/>
		</id>
		<property name="fullName" column="full_nm"/>
		<property name="age"/>
		<property name="gender"/>
		<property name="contactNo" column="contact_no"/>
		<property name="emailAddress" column="email_addr"/>
	</class>
</hibernate-mapping>