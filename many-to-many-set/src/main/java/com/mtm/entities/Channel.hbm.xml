<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mtm.entities">
	<class name="Channel" table="channel">
		<id name="channelNo" column="channel_no">
			<generator class="increment"/>
		</id>
		<property name="channelName" column="channel_nm"/>
		<property name="language" column="language"/>
		<property name="hd" column="hd"/>
		<property name="subscriptionPrice" column="subscription_price"/>
	</class>
</hibernate-mapping>